#!/bin/bash
#Установки генератора.
#Подключение резисторов или делителя напряжения (0 или 1 соответственно)
t=$1
if [ $t=1 ]; then echo Подключение делителя напряжения
	else echo Подключение резисторов
fi
run=`echo "38 38 $t 0 $t" | send2dev -d $dev -p $gener` 
if [ $? -ne 0 ]; then 
	echo Ошибка переключения
	exit 1	
fi

